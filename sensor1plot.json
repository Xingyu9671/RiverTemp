{
  "$schema": "https://vega.github.io/schema/vega/v4.json",
  "width": 800,
  "height": 500,
  "padding": 5,

  "data": [
    {
      "name": "sensor",
      "url": "https://raw.githubusercontent.com/Xingyu9671/RiverTemp/master/Sensor1.csv"
    }
  ],

  "scales":[
  	{
  		"name":"xscale",
  		"type":"band",
  		"domain":{"data":"sensor","field":"category"},
  		"range":"width",
  		"padding":0.05,
  		"round":true
  	},
  	{
  		"name":"yscale",
  		"domain":{"data":"sensor","field":"amount"},
  		"nice":true,
  		"range":"height"
  	}
  ],