{
  "$schema": "https://vega.github.io/schema/vega/v4.json",
  "width": 800,
  "height": 500,
  "padding": 5,

  "data": [
    {
      "name": "sensor",
      "url": "https://raw.githubusercontent.com/Xingyu9671/RiverTemp/master/Sensor1.csv"
    }
  ],

  "scales":[
  	{
  		"name":"xscale",
  		"type":"band",
  		"domain":{"data":"sensor","field":"Time"},
  		"range":"width",
  	},
  	{
  		"name":"yscale",
  		"type":"linear",
  		"domain":{"data":"sensor","field":"Temp"},
  		"range":"height"
  	}
  ],

  "axes":[
  	{
  		"scale":"x",
  		"grid":true,
  		"domain":false,
  		"orient":"bottom",
  		"tick"
  	}]